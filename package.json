    "vee-validate": "^4.11.1",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^2.0.1",
    "@types/node": "^18.17.6",
    "@vitejs/plugin-vue": "^4.3.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.48.0",
    "eslint-plugin-vue": "^9.17.0",
